p.js",[],"/Users/jyldyz/Desktop/art/src/Routes.jsx",[],"/Users/jyldyz/Desktop/art/src/contexts/ProductsContext.jsx",["126"],"import React, { useReducer, useState } from \"react\";\nimport axios from \"axios\";\nimport { JSON_API } from \"../helpers/constants\";\nimport { calcSubPrice, calcTotalPrice } from \"../helpers/calcPrice\";\n// import { data } from \"jquery\";\nimport { useHistory } from \"react-router-dom\";\n\nexport const productsContext = React.createContext();\n\nconst INIT_STATE = {\n  products: [],\n  productsToEdit: null,\n\n  productsCountInCart: JSON.parse(localStorage.getItem(\"cart\"))\n    ? JSON.parse(localStorage.getItem(\"cart\")).products.length\n    : 0,\n  cartData: [],\n  selectData: [],\n\n  productsCountInSelect: JSON.parse(localStorage.getItem(\"select\"))\n    ? JSON.parse(localStorage.getItem(\"select\")).products.length\n    : 0,\n\n  pageCount: 4,\n  page: 1,\n  productDetails: null,\n  totalCount: 0,\n};\n\nconst reducer = (state = INIT_STATE, action) => {\n  switch (action.type) {\n    case \"GET_PRODUCTS\":\n      return { ...state, products: action.payload };\n    case \"ADD_PRODUCT\":\n      return {\n        ...state,\n        products: [...state.products, action.payload],\n      };\n    case \"EDIT_PRODUCTS_DATA\":\n      return { ...state, productsToEdit: action.payload };\n\n    case \"SAVE_PRODUCT\":\n      return { ...state, productsToEdit: action.payload };\n\n    //CART\n    case \"ADD_AND_DELETE_CART\":\n      return { ...state, productsCountInCart: action.payload };\n    case \"GET_CART\":\n      return { ...state, cartData: action.payload };\n\n    //SELECT\n    case \"ADD_AND_DELETE_SELECT\":\n      return { ...state, productsCountInSelect: action.payload };\n    case \"GET_SELECT\":\n      return { ...state, selectData: action.payload };\n\n    // ПАГИНАЦИЯ\n    case \"CET_CONTACTS_DATA\":\n      return { ...state, products: action.payload };\n    case \"CONTACTS_SET_PAGE\":\n      return { ...state, page: action.page };\n\n    //DETAIL\n    case \"GET_PRODUCT_DETAILS\":\n      return { ...state, productDetails: action.payload };\n\n    case \"SET_TOTAL_COUNT\":\n      return { ...state, totalCount: action.payload };\n\n    //FILTER\n    case \"FILTER_PRODUCTS\":\n      return { ...state, products: action.payload };\n\n    default:\n      return state;\n  }\n};\n\nconst ProductsContextProvider = ({ children }) => {\n  const [state, dispatch] = useReducer(reducer, INIT_STATE);\n  const [inpVal, setInpVal] = useState(\"\");\n  const history = useHistory();\n\n  // SEARCH\n\n  async function getProducts() {\n    let params = window.location.search.replace(/%3D/g, \"\");\n    console.log(params);\n    let { data } = await axios(`${JSON_API}/products${params}`);\n    dispatch({\n      type: \"GET_PRODUCTS\",\n      payload: data,\n    });\n  }\n\n  const getProductsFashion = async () => {\n    let params = new URLSearchParams(window.location.search);\n    if (!params.get(\"_page\")) params.set(\"_page\", 1);\n    if (!params.get(\"_limit\")) params.set(\"_limit\", 6);\n    history.push(\"?\" + params);\n    let { data, headers } = await axios(\n      `http://localhost:8000/products?${params}`\n    );\n    dispatch({\n      type: \"SET_TOTAL_COUNT\",\n      payload: parseInt(headers[\"x-total-count\"]),\n    });\n    dispatch({\n      type: \"CET_CONTACTS_DATA\",\n      payload: data,\n    });\n  };\n\n  const getProductsAdmin = async () => {\n    let params = new URLSearchParams(window.location.search);\n    if (!params.get(\"_page\")) params.set(\"_page\", 1);\n    if (!params.get(\"_limit\")) params.set(\"_limit\", 6);\n    history.push(\"/admin?\" + params);\n    let { data, headers } = await axios(\n      `http://localhost:8000/products?${params}`\n    );\n    dispatch({\n      type: \"SET_TOTAL_COUNT\",\n      payload: parseInt(headers[\"x-total-count\"]),\n    });\n    dispatch({\n      type: \"GET_PRODUCTS\",\n      payload: data,\n    });\n  };\n\n  async function searchProducts() {\n    let params = window.location.search.replace(/%3D/g, \"\");\n    console.log(params);\n    let { data } = await axios(`${JSON_API}/products${params}`);\n    dispatch({\n      type: \"GET_PRODUCTS\",\n      payload: data,\n    });\n  }\n\n  async function getProductID(id) {\n    let { data } = await axios(`http://localhost:8000/products/${id}`);\n    dispatch({\n      type: \"GET_PRODUCT_DETAILS\",\n      payload: data,\n    });\n  }\n\n  async function addProduct(newProduct) {\n    await axios.post(\"http://localhost:8000/products\", newProduct);\n    getProductsAdmin();\n  }\n\n  async function deleteContact(id) {\n    await axios.delete(`http://localhost:8000/products/${id}`);\n    getProductsAdmin();\n  }\n\n  async function editProduct(id) {\n    const { data } = await axios.patch(`http://localhost:8000/products/${id}`);\n    dispatch({\n      type: \"EDIT_PRODUCTS_DATA\",\n      payload: data,\n    });\n  }\n\n  const saveProduct = async (newProduct, history) => {\n    console.log(newProduct, history);\n    try {\n      await axios.patch(\n        `http://localhost:8000/products/${newProduct.id}`,\n        newProduct\n      );\n\n      dispatch({\n        type: \"SAVE_PRODUCT\",\n        payload: newProduct.id,\n      });\n      getProductsAdmin();\n\n      history.push(\"/admin\");\n    } catch (error) {\n      history.push(\"/404\");\n    }\n  };\n\n  // CART\n\n  function addAndDeleteProductInCart(product) {\n    let cart = JSON.parse(localStorage.getItem(\"cart\"));\n\n    if (!cart) {\n      cart = {\n        products: [],\n        totalPrice: 0,\n      };\n    }\n\n    let newProduct = {\n      product: product,\n      count: 1,\n      subPrice: 0,\n    };\n\n    newProduct.subPrice = calcSubPrice(newProduct);\n    console.log(newProduct);\n    let newSelectCart = cart.products.filter(\n      (item) => item.product.id === product.id\n    );\n\n    if (newSelectCart.length > 0) {\n      cart.products = cart.products.filter(\n        (item) => item.product.id !== product.id\n      );\n    } else {\n      cart.products.push(newProduct);\n    }\n\n    cart.totalPrice = calcTotalPrice(cart.products);\n    localStorage.setItem(\"cart\", JSON.stringify(cart));\n\n    dispatch({\n      type: \"ADD_AND_DELETE_CART\",\n      payload: cart.products.length,\n    });\n  }\n\n  function checkProductInCart(id) {\n    let cart = JSON.parse(localStorage.getItem(\"cart\"));\n    if (!cart) {\n      cart = {\n        products: [],\n        totalPrice: 0,\n      };\n    }\n\n    let newSelectCart = cart.products.filter((item) => item.product.id === id);\n    return newSelectCart.length > 0 ? true : false;\n  }\n\n  function getCart() {\n    let cart = JSON.parse(localStorage.getItem(\"cart\"));\n    dispatch({\n      type: \"GET_CART\",\n      payload: cart,\n    });\n  }\n\n  function changeCountProducts(count, id) {\n    let cart = JSON.parse(localStorage.getItem(\"cart\"));\n    cart.products = cart.products.map((item) => {\n      if (item.product.id === id) {\n        item.count = count;\n        item.subPrice = calcSubPrice(item);\n      }\n      return item;\n    });\n\n    cart.totalPrice = calcTotalPrice(cart.products);\n    localStorage.setItem(\"cart\", JSON.stringify(cart));\n    getCart();\n  }\n\n  function makeOrder() {\n    let cart = JSON.parse(localStorage.getItem(\"cart\"));\n  }\n\n  function deleteProductInCart(id) {\n    let cart = JSON.parse(localStorage.getItem(\"cart\"));\n    console.log(id);\n    console.log(cart);\n\n    cart.products = cart.products.filter((item) => {\n      return item.product.id !== id;\n    });\n\n    localStorage.setItem(\"cart\", JSON.stringify(cart));\n    dispatch({\n      type: \"ADD_AND_DELETE_CART\",\n      payload: cart.products.length,\n    });\n    getCart();\n  }\n\n  // ПАГИНАЦИЯ\n\n  const pageTask = async (page) => {\n    const search = new URLSearchParams(window.location.search);\n    search.set(\"_page\", page);\n    search.set(\"_limit\", 6);\n    history.replace(window.location.pathname + \"?\" + search.toString());\n    getProductsFashion();\n  };\n\n  const pageAdmin = async (page) => {\n    const search = new URLSearchParams(window.location.search);\n    search.set(\"_page\", page);\n    search.set(\"_limit\", 6);\n    history.replace(window.location.pathname + \"?\" + search.toString());\n    getProductsAdmin();\n  };\n\n  //SEARCH\n\n  function search(inp_val) {\n    setInpVal(inp_val);\n    getProducts();\n  }\n\n  //FILTER\n\n  async function filterProducts(value) {\n    let params = \"\";\n    params = value ? `?category=${value}` : \"\";\n    const { data } = await axios(`${JSON_API}/products${params}`);\n    dispatch({\n      type: \"FILTER_PRODUCTS\",\n      payload: data,\n    });\n  }\n\n  // SELECT\n\n  function addAndDeleteProductInSelect(product) {\n    let select = JSON.parse(localStorage.getItem(\"select\"));\n\n    if (!select) {\n      select = {\n        products: [],\n      };\n    }\n\n    let newProduct = {\n      product: product,\n    };\n\n    let newSelectSelect = select.products.filter(\n      (item) => item.product.id === product.id\n    );\n\n    if (newSelectSelect.length > 0) {\n      select.products = select.products.filter(\n        (item) => item.product.id !== product.id\n      );\n    } else {\n      select.products.push(newProduct);\n    }\n\n    localStorage.setItem(\"select\", JSON.stringify(select));\n\n    dispatch({\n      type: \"ADD_AND_DELETE_SELECT\",\n      payload: select.products.length,\n    });\n  }\n\n  function checkProductInSelect(id) {\n    let select = JSON.parse(localStorage.getItem(\"select\"));\n    if (!select) {\n      select = {\n        products: [],\n      };\n    }\n\n    let newSelectSelect = select.products.filter(\n      (item) => item.product.id === id\n    );\n    return newSelectSelect.length > 0 ? true : false;\n  }\n\n  function getSelect() {\n    let select = JSON.parse(localStorage.getItem(\"select\"));\n    dispatch({\n      type: \"GET_SELECT\",\n      payload: select,\n    });\n  }\n\n  function deleteProductInSelect(id) {\n    let select = JSON.parse(localStorage.getItem(\"select\"));\n    console.log(id);\n\n    select.products = select.products.filter((item) => {\n      return item.product.id !== id;\n    });\n\n    localStorage.setItem(\"select\", JSON.stringify(select));\n    dispatch({\n      type: \"ADD_AND_DELETE_SELECT\",\n      payload: select.products.length,\n    });\n    getSelect();\n  }\n\n  return (\n    <productsContext.Provider\n      value={{\n        productDetails: state.productDetails,\n\n        page: state.page,\n        products: state.products,\n        productsToEdit: state.productsToEdit,\n        productsCountInCart: state.productsCountInCart,\n        cartData: state.cartData,\n\n        productsCountInSelect: state.productsCountInSelect,\n        selectData: state.selectData,\n\n        totalCount: state.totalCount,\n\n        addAndDeleteProductInCart,\n        checkProductInCart,\n        deleteProductInCart,\n\n        addAndDeleteProductInSelect,\n        checkProductInSelect,\n        getSelect,\n        deleteProductInSelect,\n\n        changeCountProducts,\n        makeOrder,\n        getProducts,\n        addProduct,\n        deleteContact,\n        editProduct,\n        saveProduct,\n        getCart,\n        pageTask,\n\n        search,\n        getProductID,\n        filterProducts,\n\n        getProductsFashion,\n        pageAdmin,\n        pageTask,\n        getProductsAdmin,\n\n        searchProducts,\n      }}\n    >\n      {children}\n    </productsContext.Provider>\n  );\n};\n\nexport default ProductsContextProvider;\n","/Users/jyldyz/Desktop/art/src/container/Home/Footer/Footer.jsx",[],"/Users/jyldyz/Desktop/art/src/container/Home/Home.jsx",[],"/Users/jyldyz/Desktop/art/src/container/Cart/Cart.jsx",[],"/Users/jyldyz/Desktop/art/src/container/AdminPage/Admin.jsx",[],"/Users/jyldyz/Desktop/art/src/contexts/AuthContext.jsx",["127"],"import React, { useReducer } from \"react\";\nimport { AUTH_API } from \"../helpers/constants\";\nimport axios from \"axios\";\n\nexport const authContext = React.createContext();\n\nconst INIT_STATE = {};\n\nconst reducer = (state = INIT_STATE, action) => {\n  switch (action.type) {\n  }\n};\n\nconst AuthContextProvider = ({ children }) => {\n  const [state, dispatch] = useReducer(reducer, INIT_STATE);\n\n  async function registerUser(e, history) {\n    e.preventDefault();\n    const newUser = {\n      email: e.target[0].value,\n      password: e.target[2].value,\n    };\n\n    try {\n      const res = await axios.post(`${AUTH_API}/api/auth/register`, newUser);\n      history.push(\"/signin\");\n    } catch {\n      alert(\"Некорректная почта или пароль\");\n    }\n  }\n\n  async function logInUser(e, history) {\n    e.preventDefault();\n    const newUser = {\n      email: e.target[0].value,\n      password: e.target[2].value,\n    };\n\n    try {\n      const res = await axios.post(`${AUTH_API}/api/auth/login`, newUser);\n      history.push(\"/\");\n    } catch {\n      alert(\"Некорректная почта или пароль\");\n    }\n  }\n\n  function hasAccount(history) {\n    history.push(\"/signin\");\n  }\n\n  function hasnotAccount(history) {\n    history.push(\"/signup\");\n  }\n\n  return (\n    <authContext.Provider\n      value={{\n        registerUser,\n        logInUser,\n        hasAccount,\n        hasnotAccount,\n      }}\n    >\n      {children}\n    </authContext.Provider>\n  );\n};\n\nexport default AuthContextProvider;\n","/Users/jyldyz/Desktop/art/src/container/Home/SignIn/SignIn.jsx",[],"/Users/jyldyz/Desktop/art/src/container/AdminPage/EditProduct.jsx",["128"],"import React, { Fragment, useContext, useEffect, useState } from \"react\";\nimport { productsContext } from \"../../contexts/ProductsContext\";\nimport { FormGroup, Label, Input, FormText } from \"reactstrap\";\nimport { Link } from \"react-router-dom\";\nimport Header from \"../../container/GlobalHeader/HeaderHome\";\n\nconst EditProduct = (props) => {\n  // console.log(props.history)\n\n  const { productsToEdit, saveProduct } = useContext(productsContext);\n\n  const [editItem, setEditItem] = useState(productsToEdit);\n\n  useEffect(() => {\n    setEditItem(productsToEdit);\n  }, [productsToEdit]);\n\n  function handle(e) {\n    let obj = {\n      ...editItem,\n      [e.target.name]: e.target.value,\n    };\n    setEditItem(obj);\n  }\n\n  return (\n    <div>\n      <Header />\n      {editItem ? (\n        <>\n          <Fragment>\n            <div className=\"container EditProduct\">\n              <h3>Add Product</h3>\n              <br />\n              <br />\n\n              <form>\n                <div className=\"form-group\">\n                  <label htmlFor=\"formGroupExampleInput\">Product Title</label>\n                  <input\n                    type=\"text\"\n                    className=\"form-control\"\n                    name=\"title\"\n                    value={editItem.title}\n                    onChange={handle}\n                  />\n                </div>\n                <br />\n\n                <div className=\"form-group\">\n                  <label htmlFor=\"formGroupExampleInput\">Description</label>\n                  <input\n                    className=\"form-control\"\n                    name=\"description\"\n                    value={editItem.description}\n                    onChange={handle}\n                  />\n                </div>\n                <br />\n\n                <div className=\"form-group\">\n                  <label htmlFor=\"formGroupExampleInput\">Price</label>\n                  <input\n                    type=\"text\"\n                    className=\"form-control\"\n                    name=\"price\"\n                    value={editItem.price}\n                    onChange={handle}\n                  />\n                </div>\n                <br />\n\n                <FormGroup>\n                  <Label for=\